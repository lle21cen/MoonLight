<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal">

    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <RelativeLayout
            android:background="@drawable/round_border_gray_no_solid"
            android:id="@+id/cash_1_layout"
            android:layout_width="150dp"
            android:layout_height="30dp">

            <RelativeLayout
                android:layout_centerInParent="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/cash_1_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1"
                    android:textColor="#1d68bf" />

                <TextView
                    android:layout_toRightOf="@id/cash_1_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" Cash"
                    android:textColor="#000000" />
            </RelativeLayout>
        </RelativeLayout>

        <RelativeLayout
            android:background="@drawable/round_border_gray_no_solid"
            android:id="@+id/cash_50_layout"
            android:layout_marginTop="5dp"
            android:layout_width="150dp"
            android:layout_height="30dp">

            <RelativeLayout
                android:layout_centerInParent="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/cash_50_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="50"
                    android:textColor="#1d68bf" />

                <TextView
                    android:layout_toRightOf="@id/cash_50_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" Cash"
                    android:textColor="#000000" />
            </RelativeLayout>

        </RelativeLayout>
        <RelativeLayout
            android:background="@drawable/round_border_gray_no_solid"
            android:id="@+id/cash_200_layout"
            android:layout_marginTop="5dp"
            android:layout_width="150dp"
            android:layout_height="30dp">

            <RelativeLayout
                android:layout_centerInParent="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/cash_200_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="200"
                    android:textColor="#1d68bf" />

                <TextView
                    android:layout_toRightOf="@id/cash_200_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" Cash"
                    android:textColor="#000000" />
            </RelativeLayout>

        </RelativeLayout>
        <RelativeLayout
            android:background="@drawable/round_border_gray_no_solid"
            android:id="@+id/cash_1000_layout"
            android:layout_width="150dp"
            android:layout_marginTop="5dp"
            android:layout_height="30dp">

            <RelativeLayout
                android:layout_centerInParent="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/cash_1000_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="1000"
                    android:textColor="#1d68bf" />

                <TextView
                    android:layout_toRightOf="@id/cash_1000_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" Cash"
                    android:textColor="#000000" />
            </RelativeLayout>

        </RelativeLayout>

    </LinearLayout>

    <LinearLayout
        android:layout_marginLeft="10dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical">

        <RelativeLayout
            android:background="@drawable/round_border_gray_no_solid"
            android:id="@+id/cash_10_layout"
            android:layout_width="150dp"
            android:layout_height="30dp">

            <RelativeLayout
                android:layout_centerInParent="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/cash_10_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="10"
                    android:textColor="#1d68bf" />

                <TextView
                    android:layout_toRightOf="@id/cash_10_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" Cash"
                    android:textColor="#000000" />
            </RelativeLayout>
        </RelativeLayout>

        <RelativeLayout
            android:background="@drawable/round_border_gray_no_solid"
            android:id="@+id/cash_100_layout"
            android:layout_marginTop="5dp"
            android:layout_width="150dp"
            android:layout_height="30dp">

            <RelativeLayout
                android:layout_centerInParent="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/cash_100_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="100"
                    android:textColor="#1d68bf" />

                <TextView
                    android:layout_toRightOf="@id/cash_100_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" Cash"
                    android:textColor="#000000" />
            </RelativeLayout>

        </RelativeLayout>
        <RelativeLayout
            android:background="@drawable/round_border_gray_no_solid"
            android:id="@+id/cash_500_layout"
            android:layout_marginTop="5dp"
            android:layout_width="150dp"
            android:layout_height="30dp">

            <RelativeLayout
                android:layout_centerInParent="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

                <TextView
                    android:id="@+id/cash_500_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="500"
                    android:textColor="#1d68bf" />

                <TextView
                    android:layout_toRightOf="@id/cash_500_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text=" Cash"
                    android:textColor="#000000" />
            </RelativeLayout>

        </RelativeLayout>
        <RelativeLayout
            android:background="@drawable/round_border_yellow"
            android:id="@+id/cash_reset"
            android:layout_width="150dp"
            android:layout_marginTop="5dp"
            android:layout_height="30dp">

            <TextView
                android:text="초기화"
                android:textColor="#000000"
                android:layout_centerInParent="true"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />

        </RelativeLayout>

    </LinearLayout>

</LinearLayout>